import { Injectable } from '@angular/core';

@Injectable({
  providedIn: 'root'
})
export class ConfigService {
  private _config!: Config;

  constructor() { }

  public set config(v : Config) {
    this._config = v;
  }
  
  public get config() : Config {
    return this._config;
  }
}

export interface Config {
  GATEWAY_URL: string;
  ORG_DN: string;
  AUTH_TYPE: string;
  OTDS_URL?: string;
  UI_FRAMEWORK?: string;
}
