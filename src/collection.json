{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "description": "Add AppWorks to the project.",
      "factory": "./ng-add/index#ngAdd",
      "schema": "./app-works/schema.json"
    },
    "app-works": {
      "description": "Schematic to configure AppWorks environment",
      "factory": "./app-works/index#appWorks",
      "schema": "./app-works/schema.json"
    },
    "service": {
      "description": "Schematic to add AppWorks webservice",
      "factory": "./service/index#service",
      "schema": "./service/schema.json"
    },
    "component": {
      "description": "Create service for the wsdl and a component using the service",
      "factory": "./component/index#component",
      "schema": "./component/schema.json"
    },
    "login": {
      "description": "Add login page schematic.",
      "factory": "./login/index#login",
      "schema": "./login/schema.json"
    },
    "authenticate": {
      "description": "Add authentication support schematic.",
      "factory": "./authenticate/index#authentication",
      "schema": "./authenticate/schema.json"
    }
  }
}